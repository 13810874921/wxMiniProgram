/*compare common  */

.format {
  border-bottom: 1px solid #f2f2f2;
  padding-bottom: 28px;
  margin: 0 16px 28px;
}

.up {
  color: #3d9300;
}

.down {
  color: #c73e3a;
}

.debug {
  margin-top: 120px;
}

.title {
  font-weight: 600;
  font-size: 20px;
  padding-bottom: 28px;
  height:26px;
}

.sz {
  font-size: 16px;
}

.p50 {
  width: 50%;
}

.mtb10 {
  margin: 10px 0;
}

.defaultColor {
  color: #979797;
  font-size: 13px;
}

.defaultText {
  font-size: 17px;
  font-weight: 600;
}

.exclusive {
  color: #fff;
  padding: 1px 4px;
  font-size: 13px;
  background: #fe5000;
  line-height: 20px;
  font-weight: 400;
  border-radius: 2px;
  margin: 3px 10px 0;
}

.relative {
  position: relative;
}

.i {
  width: 22px;
  height: 22px;
  vertical-align: text-bottom;
  margin-top: 3px;
}

/*others*/

.font {
  width: 50%;
  border-bottom: 0;
  height: 60px;
  overflow: hidden;
}

/* .vs{width:20%;float:left;line-height:60px;font-size:20px;} */

.search {
  width: 50%;
  color: #ff5100;
  font-size: 13px;
  margin: 0 0 15px;
}

.compareView {
  border-bottom: 1px solid #f2f2f2;
  position: fixed;
  width: 100%;
  top: 0;
  background: #fff;
}

.compareHead {
  margin: 0 16px 0;
}

.compareView:before {
  content: '';
  height: 0;
  background: transparent;
  display: block;
}

/*baseComponent  */

.base {
  border: 4px solid #293995;
  width: 30vw;
  height: 30vw;
  border-radius: 50%;
  padding-top: 20px;
}

.base1 {
  border: 4px solid #2672db;
}

.base text {
  color: #293995;
}

.base1 text {
  color: #2672db;
}

.shareBtn {
  width: 106px;
  height: 36px;
  background: #fe5000;
  line-height: 36px;
  color: #fff;
  border-radius: 100px;
  position: fixed;
  bottom: 30px;
  left: 0;
  right: 0;
}

.shareImg {
  position: absolute;
  width: 20px;
  height: 20px;
  left: 20px;
  top: 8px;
}

.shareBtn text {
  padding-left: 16px;
  font-size: 17px;
}
